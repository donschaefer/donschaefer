    "deploy": "gh-pages -d build",
    "start": "eslint src/** --fix && react-scripts start",
    "build": "eslint src/** --fix && react-scripts test --verbose --watchAll=false ./src/tests/unitTests && react-scripts build",
    "test": "react-scripts test --verbose --watchAll=false ./src/tests/unitTests",
    "eject": "react-scripts eject",
    "build-for-local": "react-scripts start",
    "build-for-server": "react-scripts build",
    "fix": "eslint src/** --fix",
    "test-logic": "react-scripts test --verbose --watchAll=false ./src/tests/unitTests",
    "test-browsers": "playwright test --browser=all",
    "test-browser": "playwright test --headed"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@mui/types": "^7.2.0",
    "@types/react-dom": "^18.0.8",
    "@types/react-router-dom": "^5.3.3",
    "@types/sass": "^1.43.1",
    "@types/video.js": "^7.3.49",
    "@types/videojs": "^5.16.2",
